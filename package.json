{
  "name": "@aws-samples/smart-product-onboarding",
  "scripts": {
    "build": "nx run-many --target=build --output-style=stream --nx-bail",
    "clobber": "git checkout -b scratch && git checkout $(git branch --show-current) && git fetch origin && git reset --hard origin/$(git branch --show-current) && git clean -fdx",
    "compile": "nx run-many --target=compile --output-style=stream --nx-bail",
    "lint": "nx run-many --target=eslint --output-style=stream --nx-bail",
    "graph": "nx graph",
    "package": "nx run-many --target=package --output-style=stream --nx-bail",
    "post-compile": "nx run-many --target=post-compile --output-style=stream --nx-bail",
    "postinstall": "nx run-many --target=install --projects=tag:python --output-style=stream --parallel=1 --nx-bail",
    "pre-compile": "nx run-many --target=pre-compile --output-style=stream --nx-bail",
    "run-many": "nx run-many",
    "test": "nx run-many --target=test --output-style=stream --nx-bail",
    "watch": "nx run-many --target=watch --output-style=stream --skip-nx-cache --nx-ignore-cycles --nx-bail",
    "install:ci": "pnpm i --frozen-lockfile && nx run-many --target=install:ci --output-style=stream --nx-bail"
  },
  "devDependencies": {
    "@aws/pdk": "0.26.7",
    "@types/node": "^18",
    "@typescript-eslint/eslint-plugin": "^7",
    "@typescript-eslint/parser": "^7",
    "eslint": "^8",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "npm-check-updates": "^19.0.0",
    "nx": "19.0.0",
    "prettier": "^3.6.2",
    "syncpack": "^13",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "nx": "^19"
  },
  "dependencies": {
    "@aws-cdk/aws-cognito-identitypool-alpha": "latest",
    "aws-cdk-lib": "^2.219.0",
    "cdk-nag": "^2.37.49"
  },
  "pnpm": {
    "overrides": {
      "@babel/runtime-corejs3": "^7.26.10",
      "@babel/runtime": "^7.26.10",
      "@types/babel__traverse": "7.18.2",
      "@zkochan/js-yaml": "npm:js-yaml@4.1.0",
      "aws-cdk-lib": "^2.195.0",
      "axios": "^1.12.0",
      "brace-expansion": "^1.1.12",
      "cross-spawn": "^7.0.5",
      "dompurify": "^3.1.7",
      "esbuild": "^0.25.0",
      "fast-xml-parser": "^4.4.1",
      "form-data": "^3.0.4",
      "http-proxy-middleware": "^2.0.8",
      "micromatch": "^4.0.8",
      "nth-check": "^2.1.1",
      "on-headers": "^1.1.0",
      "postcss": "^8.4.38",
      "prismjs": "^1.30.0",
      "semver": "^7.6.2",
      "serialize-javascript": "^6.0.2",
      "sha.js": "^2.4.12",
      "tmp": "^0.2.4",
      "webpack-dev-server": "^4.15.1",
      "webpack": "^5.94.0",
      "wrap-ansi": "^7.0.0",
      "ws": "^8.17.1"
    }
  },
  "engines": {
    "node": ">=16",
    "pnpm": ">=8"
  },
  "main": "lib/index.js",
  "license": "MIT-0",
  "version": "0.0.0",
  "types": "lib/index.d.ts",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/api/handlers/typescript",
      "packages/api/generated/infrastructure/typescript",
      "packages/api/generated/libraries/typescript-react-query-hooks",
      "packages/api/generated/runtime/typescript",
      "packages/website",
      "packages/infra"
    ]
  }
}
