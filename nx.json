{
  "extends": "nx/presets/npm.json",
  "npmScope": "monorepo",
  "affected": {
    "defaultBase": "main"
  },
  "namedInputs": {
    "default": ["{projectRoot}/**/*"],
    "typescript": [
      "{projectRoot}/**/*.ts",
      "{projectRoot}/**/*.tsx",
      "{projectRoot}/**/*.js",
      "{projectRoot}/**/*.jsx",
      "{projectRoot}/tsconfig.json",
      "{projectRoot}/tsconfig.*.json"
    ],
    "python": [
      "{projectRoot}/**/*.py",
      "{projectRoot}/pyproject.toml",
      "{projectRoot}/poetry.lock",
      "{projectRoot}/poetry.toml"
    ]
  },
  "targetDefaults": {
    "build": {
      "inputs": ["default", "^default"],
      "outputs": [
        "{projectRoot}/dist",
        "{projectRoot}/lib",
        "{projectRoot}/build",
        "{projectRoot}/coverage",
        "{projectRoot}/test-reports",
        "{projectRoot}/target",
        "{projectRoot}/cdk.out",
        "{projectRoot}/LICENSE_THIRD_PARTY",
        "{projectRoot}/.jsii"
      ],
      "dependsOn": ["^build"],
      "env": {
        "AWS_PDK_VERSION": "0.26.7"
      }
    },
    "test": {
      "inputs": ["default", "^default"],
      "outputs": [
        "{projectRoot}/coverage",
        "{projectRoot}/test-reports",
        "{projectRoot}/.nyc_output"
      ],
      "dependsOn": ["compile"],
      "env": {
        "AWS_PDK_VERSION": "0.26.7"
      }
    },
    "lint": {
      "inputs": ["default"],
      "env": {
        "AWS_PDK_VERSION": "0.26.7"
      }
    },
    "package": {
      "inputs": ["default", "^default"],
      "outputs": [
        "{projectRoot}/dist",
        "{projectRoot}/lib",
        "{projectRoot}/target",
        "{projectRoot}/*.tgz",
        "{projectRoot}/*.whl",
        "{projectRoot}/*.tar.gz"
      ],
      "dependsOn": ["build"],
      "env": {
        "AWS_PDK_VERSION": "0.26.7"
      }
    },
    "compile": {
      "inputs": ["default", "^default"],
      "outputs": [
        "{projectRoot}/dist",
        "{projectRoot}/lib",
        "{projectRoot}/build"
      ],
      "dependsOn": ["^compile"],
      "env": {
        "AWS_PDK_VERSION": "0.26.7"
      }
    },
    "install": {
      "inputs": [
        "{projectRoot}/pyproject.toml",
        "{projectRoot}/poetry.lock",
        "{projectRoot}/package.json",
        "{projectRoot}/pnpm-lock.yaml"
      ],
      "dependsOn": ["^install"],
      "env": {
        "PATH": "{env.PATH}:/usr/local/bin",
        "AWS_PDK_VERSION": "0.26.7"
      }
    },
    "synth": {
      "inputs": ["default", "^default"],
      "outputs": ["{projectRoot}/cdk.out"],
      "dependsOn": ["^build"],
      "env": {
        "AWS_PDK_VERSION": "0.26.7"
      }
    },
    "deploy": {
      "inputs": ["default", "^default"],
      "dependsOn": ["synth"],
      "env": {
        "AWS_PDK_VERSION": "0.26.7"
      }
    }
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "useDaemonProcess": false,
        "cacheableOperations": [
          "build",
          "test",
          "lint",
          "package",
          "compile",
          "install"
        ]
      }
    }
  }
}
