{
  "app": "npx ts-node -P tsconfig.json --prefer-ts-exts src/main.ts",
  "context": {
    "wordVectorsUrl": "https://dl.fbaipublicfiles.com/fasttext/vectors-english/crawl-300d-2M.vec.zip"
  },
  "output": "cdk.out",
  "build": "nx run @aws-samples/smart-product-onboarding-infra:compile",
  "watch": {
    "include": ["src/**/*.ts", "test/**/*.ts"],
    "exclude": [
      "README.md",
      "cdk*.json",
      "**/*.d.ts",
      "**/*.js",
      "tsconfig.json",
      "package*.json",
      "yarn.lock",
      "node_modules"
    ]
  }
}
