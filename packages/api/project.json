{
  "name": "@aws-samples/smart-product-onboarding-api",
  "root": "packages/api",
  "targets": {
    "pre-compile": {
      "executor": "nx:run-commands",
      "options": {
        "command": "scripts/run-task pre-compile",
        "cwd": "packages/api"
      }
    },
    "compile": {
      "executor": "nx:run-commands",
      "options": {
        "command": "scripts/run-task compile",
        "cwd": "packages/api"
      }
    },
    "post-compile": {
      "executor": "nx:run-commands",
      "options": {
        "command": "scripts/run-task post-compile",
        "cwd": "packages/api"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "scripts/run-task test",
        "cwd": "packages/api"
      }
    },
    "package": {
      "executor": "nx:run-commands",
      "options": {
        "command": "scripts/run-task package",
        "cwd": "packages/api"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "scripts/run-task build",
        "cwd": "packages/api"
      }
    }
  }
}
