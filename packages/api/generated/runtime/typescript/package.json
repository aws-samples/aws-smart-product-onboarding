{
  "name": "@aws-samples/smart-product-onboarding-api-typescript-runtime",
  "scripts": {
    "build": "echo 'Use nx run @aws-samples/smart-product-onboarding-api-typescript-runtime:build instead'",
    "compile": "tsc --build",
    "generate": "pnpm exec type-safe-api generate --specPath ../../../model/.api.json --outputPath . --templateDirs \"typescript\" --metadata '{\"srcDir\":\"src\"}'",
    "package": "mkdir -p dist/js && pnpm pack --pack-destination dist/js",
    "test": "echo 'No tests defined for generated runtime package'",
    "watch": "tsc --build -w"
  },
  "devDependencies": {
    "@aws/pdk": "^0",
    "@types/node": "^18",
    "typescript": "*"
  },
  "dependencies": {
    "@aws-lambda-powertools/logger": "*",
    "@aws-lambda-powertools/metrics": "*",
    "@aws-lambda-powertools/tracer": "*",
    "@types/aws-lambda": "*"
  },
  "main": "lib/index.js",
  "license": "Apache-2.0",
  "version": "0.0.0",
  "types": "lib/index.d.ts"
}
