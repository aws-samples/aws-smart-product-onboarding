{
  "name": "@aws-samples/smart-product-onboarding-api-typescript-infra",
  "scripts": {
    "build": "echo 'Use nx run @aws-samples/smart-product-onboarding-api-typescript-infra:build instead'",
    "compile": "tsc --build",
    "generate": "pnpm exec type-safe-api generate --specPath ../../../model/.api.json --outputPath . --templateDirs \"typescript-cdk-infrastructure\" --metadata '{\"srcDir\":\"src\",\"runtimePackageName\":\"@aws-samples/smart-product-onboarding-api-typescript-runtime\",\"relativeSpecPath\":\"../assets/api.json\",\"enableMockIntegrations\":true,\"x-handlers-python-module\":\"amzn_smart_product_onboarding_api_python_handlers\",\"x-handlers-java-package\":\"\",\"x-handlers-typescript-asset-path\":\"../../../handlers/typescript/dist/lambda\",\"x-handlers-python-asset-path\":\"../../../handlers/python/dist/lambda\",\"x-handlers-java-asset-path\":\"\",\"x-handlers-node-lambda-runtime-version\":\"NODEJS_22_X\",\"x-handlers-python-lambda-runtime-version\":\"PYTHON_3_13\",\"x-handlers-java-lambda-runtime-version\":\"\"}'",
    "package": "mkdir -p dist/js && pnpm pack --pack-destination dist/js",
    "test": "echo 'No tests defined for generated infrastructure package'",
    "watch": "tsc --build -w"
  },
  "devDependencies": {
    "@aws/pdk": "^0",
    "@types/aws-lambda": "*",
