{
  "name": "@aws-samples/smart-product-onboarding-api-documentation-markdown",
  "root": "packages/api/generated/documentation/markdown",
  "targets": {
    "pre-compile": {
      "executor": "nx:run-commands",
      "options": {
        "command": "scripts/run-task pre-compile",
        "cwd": "packages/api/generated/documentation/markdown"
      }
    },
    "compile": {
      "executor": "nx:run-commands",
      "options": {
        "command": "scripts/run-task compile",
        "cwd": "packages/api/generated/documentation/markdown"
      }
    },
    "post-compile": {
      "executor": "nx:run-commands",
      "options": {
        "command": "scripts/run-task post-compile",
        "cwd": "packages/api/generated/documentation/markdown"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "scripts/run-task test",
        "cwd": "packages/api/generated/documentation/markdown"
      }
    },
    "package": {
      "executor": "nx:run-commands",
      "options": {
        "command": "scripts/run-task package",
        "cwd": "packages/api/generated/documentation/markdown"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "scripts/run-task build",
        "cwd": "packages/api/generated/documentation/markdown"
      }
    },
    "generate": {
      "executor": "nx:run-commands",
      "options": {
        "command": "scripts/run-task generate",
        "cwd": "packages/api/generated/documentation/markdown"
      }
    }
  },
  "implicitDependencies": [
    "@aws-samples/smart-product-onboarding-api-model"
  ]
}
